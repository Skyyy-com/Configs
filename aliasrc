alias grep="grep --color=auto"
alias yta="youtube-dl -ic --extract-audio --audio-format=mp3"
alias ytaw="youtube-dl -ic --extract-audio --audio-format=wav"
alias ..="cd ..&&pwd"
alias ll="ls -lah"
alias spotdl="python3 -m spotdl"
alias torb="cd ~/Downloads/tor-browser_en-US/ && ./start-tor-browser.desktop && cd"
alias du="du -h"
alias cl="cd && clear"
alias cls="clear && ls"
alias screencapture="ffmpeg -f x11grab -s 1366x768 -i :0.0 out.mp4"
alias open="nemo ."
